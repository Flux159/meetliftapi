<% layout('layout') -%>

<h1>All attempts</h1>

<ul>
	<% attempts.forEach(function(attempt) { %>
    <li>  <%= attempt.CompPart.Person.person_first_name %>
        <%= attempt.CompPart.Person.person_last_name %>
        <%= attempt.CompPart.team %>  <%= attempt.attempt_num %> <%= attempt.attempt_weight %>  </li>
  <% }); %>
</ul>